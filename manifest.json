{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Knoteup",
    "short_name": "Knoteup",
    "description": "Knoteup from your New Tab window",
    "chrome_url_overrides": {
        "newtab": "app/newtab.html"
    },

    "icons": {
      "16": "app/images/tomato.png"
    },

    "permissions": [
        "<all_urls>",
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "app/images/tomato.png",
        "default_title": "Knoteup",
        "default_popup": "app/popup.html"
    }
}
